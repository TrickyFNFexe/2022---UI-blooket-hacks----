{
    "manifest_version": 2,
    "name": "Blooket Hacks Hacking god",
    "description": "Blooket_Hacks For MBHV",
    "author": "MBHV : Mememan#9977",
    "version": "6.9.0",
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "debugger",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.blooket.com/*"
            ],
            "js": [
                "content.js",
                "jwt-decode.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Blooket Hack"
    },
    "web_accessible_resources": [
        "*ata.js",
        "scripts/*",
        "css/*"
    ],
    "icons": {
        "1024": "icons/1024.png"
    }
}